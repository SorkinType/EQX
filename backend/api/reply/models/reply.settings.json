{
  "kind": "collectionType",
  "collectionName": "replies",
  "info": {
    "name": "Reply"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "content": {
      "type": "text"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "replies"
    },
    "comment": {
      "model": "comment",
      "via": "replies"
    }
  }
}
