{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "questions",
  "info": {
    "name": "question",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": "",
    "draftAndPublish": false
  },
  "attributes": {
    "editor_image": {
      "type": "json"
    },
    "image_type": {
      "enum": [
        "uploaded_image",
        "code_image",
        "editor_image"
      ],
      "type": "enumeration"
    },
    "context": {
      "type": "text"
    },
    "question": {
      "type": "string"
    },
    "uploaded_image": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "published": {
      "default": true,
      "type": "boolean"
    },
    "test": {
      "via": "questions",
      "model": "test"
    },
    "source": {
      "type": "string"
    },
    "code_image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "code_data": {
      "type": "json"
    },
    "reference_images": {
      "type": "component",
      "repeatable": true,
      "component": "images.reference-images"
    },
    "comments": {
      "via": "question",
      "collection": "comment"
    },
    "answers": {
      "via": "question",
      "collection": "answer"
    }
  }
}
