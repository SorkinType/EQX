{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "tests",
  "info": {
    "name": "test",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "tags": {
      "type": "json"
    },
    "major_version": {
      "type": "integer",
      "required": true,
      "default": "0"
    },
    "minor_version": {
      "type": "integer",
      "min": 1,
      "default": "1",
      "required": true
    },
    "questions": {
      "collection": "question"
    },
    "completness": {
      "type": "float",
      "default": "0"
    },
    "archived": {
      "type": "boolean",
      "default": false
    },
    "users": {
      "via": "tests",
      "plugin": "users-permissions",
      "collection": "user"
    },
    "owners": {
      "via": "owned_tests",
      "plugin": "users-permissions",
      "collection": "user"
    }
  }
}
