{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "tests",
  "info": {
    "name": "test",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "tags": {
      "type": "json"
    },
    "major_version": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "minor_version": {
      "type": "integer",
      "default": 1,
      "required": true
    },
    "questions": {
      "via": "test",
      "collection": "question"
    },
    "completeness": {
      "type": "float"
    },
    "archived": {
      "type": "boolean",
      "default": false
    },
    "users": {
      "plugin": "users-permissions",
      "collection": "user",
      "via": "tests"
    },
    "owners": {
      "plugin": "users-permissions",
      "collection": "user",
      "via": "owned_tests"
    },
    "project": {
      "model": "project",
      "via": "tests"
    },
    "description": {
      "type": "richtext"
    }
  }
}
