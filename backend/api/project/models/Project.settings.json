{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "projects",
  "info": {
    "name": "project",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "name": {
      "required": true,
      "type": "string"
    },
    "major_version": {
      "type": "integer",
      "default": "0"
    },
    "minor_version": {
      "type": "integer",
      "default": "1"
    },
    "fonts": {
      "collection": "font"
    },
    "tags": {
      "collection": "tag",
      "via": "projects",
      "dominant": true
    },
    "archived": {
      "type": "boolean",
      "default": false
    },
    "tests": {
      "collection": "test"
    },
    "users": {
      "via": "projects",
      "plugin": "users-permissions",
      "collection": "user"
    },
    "owners": {
      "via": "owned_projects",
      "plugin": "users-permissions",
      "collection": "user"
    }
  }
}
