{
  "connection": "default",
  "collectionName": "projects",
  "info": {
    "name": "project",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "tests": {
      "collection": "test"
    },
    "users": {
      "collection": "user",
      "plugin": "users-permissions"
    },
    "name": {
      "required": true,
      "type": "string"
    },
    "major_version": {
      "type": "integer",
      "default": "0"
    },
    "minor_version": {
      "type": "integer",
      "default": "1"
    },
    "fonts": {
      "collection": "font"
    },
    "tags": {
      "collection": "tag"
    }
  }
}